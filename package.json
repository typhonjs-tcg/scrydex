{
  "name": "@typhonjs-tcg/scrydex",
  "version": "0.0.0",
  "description": "Tools for working with Scryfall and CSV card collection data from ManaBox and more.",
  "type": "module",
  "bin": "./dist-npm/cli/index.js",
  "author": "Michael Leahy <support@typhonjs.io> (https://github.com/typhonrt)",
  "contributors": [
    "Michael Leahy <support@typhonjs.io> (https://github.com/typhonrt)"
  ],
  "license": "MPL-2.0",
  "imports": {
    "#scrydex": {
      "types": "./src/index.ts",
      "default": "./src/index.ts",
      "replace": "@typhonjs-tcg/scrydex"
    },
    "#scrydex/*": {
      "types": "./src/*/index.ts",
      "default": "./src/*/index.ts",
      "replace": "@typhonjs-tcg/scrydex/*"
    }
  },
  "exports": {
    ".": {
      "types": "./dist-npm/index.d.ts",
      "import": "./dist-npm/index.js"
    },
    "./commands": {
      "types": "./dist-npm/commands/index.d.ts",
      "import": "./dist-npm/commands/index.js"
    },
    "./data/db": {
      "types": "./dist-npm/data/db/index.d.ts",
      "import": "./dist-npm/data/db/index.js"
    },
    "./data/db/util": {
      "types": "./dist-npm/data/db/util/index.d.ts",
      "import": "./dist-npm/data/db/util/index.js"
    },
    "./data/import": {
      "types": "./dist-npm/data/import/index.d.ts",
      "import": "./dist-npm/data/import/index.js"
    },
    "./data/scryfall": {
      "types": "./dist-npm/data/scryfall/index.d.ts",
      "import": "./dist-npm/data/scryfall/index.js"
    },
    "./data/sort": {
      "types": "./dist-npm/data/sort/index.d.ts",
      "import": "./dist-npm/data/sort/index.js"
    },
    "./util": {
      "types": "./dist-npm/util/index.d.ts",
      "import": "./dist-npm/util/index.js"
    }
  },
  "scripts": {
    "eslint": "eslint .",
    "build-exec": "bun scripts/build-exec.js",
    "build-npm": "rollup -c scripts/build-npm.js",
    "check": "esm-d-ts check src/cli/index.ts -t ./tsconfig.json",
    "docs": "typedoc-pkg --api-link es",
    "test-convert": "bun ./src/cli/index.ts convert-csv ./test/fixture/data --output ./test/fixture/output/collection-small.json --db ./test/fixture/data/db-small.json --loglevel debug",
    "test-export": "bun ./src/cli/index.ts export-csv ./test/fixture/output/collection-small.json --output ./test/fixture/output/collection-small.csv --loglevel debug",
    "test-filter": "bun ./src/cli/index.ts filter ./test/fixture/output/collection-small.json --border black:borderless --color-identity {W}{U}{G} --formats premodern --output ./test/fixture/output/collection-small-wug.json --loglevel debug",
    "test-sort": "bun ./src/cli/index.ts sort-format ./test/fixture/output/collection-small.json --formats premodern:oldschool:predh:commander --output ./test/fixture/output/sorted --loglevel debug"
  },
  "dependencies": {
    "@typhonjs-utils/file-util": "^0.1.0",
    "@typhonjs-utils/logger-color": "^0.2.0",
    "@typhonjs-utils/object": "^0.5.0",
    "csv-parse": "^6.1.0",
    "csv-stringify": "^6.6.0",
    "exceljs": "^4.4.0",
    "sade": "^1.8.0",
    "stream-chain": "^3.4.0",
    "stream-json": "^1.9.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/stream-json": "^1.7.8",
    "@typhonjs-build-test/esm-d-ts": "^0.3.0-next.14",
    "@typhonjs-build-test/rollup-plugin-pkg-imports": "^0.7.0",
    "@typhonjs-typedoc/typedoc-pkg": "^0.4.0",
    "@typhonjs-config/eslint-config": "^0.6.0",
    "bun": "^1.3.0",
    "eslint": "^8.57.0",
    "rollup": "^4.54.0",
    "tslib": "^2.8.0",
    "typescript": "^5.9.3"
  }
}
